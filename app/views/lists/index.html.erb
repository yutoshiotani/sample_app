<h1>投稿一覧</h1>

<% @lists.each do |list| %>

<p>タイトル</p>
<!-- <span></span> を削除して、以下コードに変更 -->
<span>
  <%= link_to list.title, "/list_pass(list.id)"%>
  </span>
  
  <p>画像</p>
  <% if list.image.attached? %>
  <%= image_tag list.image, size: "200×200" %>
  <% else %>
   <%= image_tag 'no_image', size: "200×200"%>
  <% end %>
   

<% end %>